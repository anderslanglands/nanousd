PROJECT_NAME = nanousd

GENERATE_TREEVIEW      = YES # optional. Also works without treeview
DISABLE_INDEX = NO
FULL_SIDEBAR = NO
HTML_EXTRA_STYLESHEET  = @AWESOME_CSS_DIR@/doxygen-awesome.css
HTML_COLORSTYLE        = LIGHT # required with Doxygen >= 1.9.5

GENERATE_XML = YES

EXCLUDE = @PROJECT_SOURCE_DIR@/CLAUDE.md @PROJECT_SOURCE_DIR@/DESIGN_NOTES.md

INPUT = @PROJECT_SOURCE_DIR@
OUTPUT_DIRECTORY = @PROJECT_SOURCE_DIR@/python/docs/_build/doxygen